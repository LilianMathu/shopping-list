  "license": "ISC",
  "dependencies": {
    "body-parser": "1.20.1",
    "concurrently": "4.1.1",
    "express": "4.18.2",
    "mongoose": "5.6.5",
    "uuidv4": "4.0.0"
  },
  "devDependencies": {
    "nodemon": "1.19.1"
  }
}
